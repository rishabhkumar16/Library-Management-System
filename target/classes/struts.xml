<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" extends="struts-default" namespace="/">
		<action name="getUser" class="com.sdp.action.SignInAction">
			<result name="success">/admin.html</result>
			<result name="login">/signin.html</result>
		</action>
		<action name="setUser" class="com.sdp.action.SignUpAction">
			<result name="success">/signin.html</result>
		</action>
	</package>
</struts>